                $(this).addClass('zzimOn');
                $(this).children('span').remove();
                var zzimApiUrl = '/api/rest/v4/wishList/addToWishList';
                var apiData = { pageId : pageId };

                $.ajax({
                    type: 'post',
                    url: zzimApiUrl,
                    data: apiData,
                    dataType: 'json',
                    cache: false
                }).done(function(jqXHR) {
                    // Segment Analytics: Tracks when user adds product to wishlist
