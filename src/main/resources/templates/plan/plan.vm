                        $('#zzimCheck' + pageId).hide();
                        disableZzim = false;
                    }, 1000);
                    return false;
                }).fail(function(jqXHR) {
                    return false;
                }).always(function() {
                    return false;
                });
            } else {
                $(this).removeClass('zzimOn');
                $(this).append('<span class="zzimTag"></span>');
                var zzimApiUrl = '/api/rest/v4/wishList/removeWishListItemByUser';
                var apiData = { pageId : pageId };

                $.ajax({
                    type: 'post',
                    url: zzimApiUrl,
                    data: apiData,
                    dataType: 'json',
                    cache: false
                }).done(function(jqXHR) {
                    // Segment Analytics: Tracks when user adds product to wishlist
                    analytics.track('Wishlist Removed', {
                        id: pageId,
                        name: pageTitle
                    });
                    return false;
                }).fail(function(jqXHR) {
                    return false;
                }).always(function() {
                    return false;
                });
            }
        }

        return false;
    });

