		/**
		 *  기본정보
		 */
		var maxImageCount = 4;
		var sumImageSize = 0;
		var maxImageSize = 10 * 1024 * 1024; // 10M

		/**
		 * 첨부파일버튼 추가
		 */
		var addNewInputFile = function() {
			var newFileInput = '<input class="find fL" type="file" style="border:none;border-right:0px; border-top:0px; boder-left:0px; boder-bottom:0px;" id="fileId" name="filename"/>';
			$('#imageUploadWrapper').append(newFileInput);
		};

		/**
		 * input file 이벤트
		 */
		$(document)
				.on(
						'change',
						'.find',
						function(event) {

							event.preventDefault();

							// Image Size 추출
							var imageSize = this.files[0].size;

							// Image Size Check
							if (sumImageSize + imageSize > maxImageSize) {

								alert('첨부 사진은 최대 10M까지 지원합니다.');

								// $( '#imageupload' + $(this).attr('name') ).remove();
								$('input[name="filename"]').remove();

								addNewInputFile();

								return false;
							}
							sumImageSize += imageSize;

							// 현재 날짜 취득 (이름에 사용)
							var date = new Date();
							// 파일 이름
							var filename = this.files[0].name;
							filename = encodeURIComponent(filename);
							// 파일 이름 + 현재 날짜
							var filenameDate = this.files[0].name
									+ date.getTime();
							filenameDate = encodeURIComponent(filenameDate);
							$(this).attr('name', filenameDate);

							// 파일추가
							var image = '<div name="' + filenameDate + '" id="image">'
									+ '<p class="fileName fL">'
									+ filename
									+ '<button class="x" type="button" name="' + filenameDate + '" fileSize="' + imageSize + '"></button></p></div>';

							$('#imageListWrapper').append(image);

							// input files tag 추가
							$('#imageUploadWrapper input:file').hide();

							if ($('#imageUploadWrapper input:file').length < 4) {
								// input files 추가
								addNewInputFile();
							}
						});

		/**
		 * .x button click event
		 */
		$(document)
				.on(
						'click',
						'.x',
						function(event) {

							event.preventDefault();

							//target
							var target = $(this).attr('value');
							// filename 취득
							var fileName = $(this).attr("name");
							// img 삭제
							$('div[name="' + fileName + '"]').remove();
							// input file 삭제
							$('input[name="' + fileName + '"]').remove();

							var imageSize = Number($(this).attr('fileSize'));
							sumImageSize -= imageSize;

							// 파일추가
							if ($('#imageUploadWrapper input:file').length < maxImageCount
									&& $('input[name="filename"]').length === 0) {
								addNewInputFile();
							}
						});
	</script>


